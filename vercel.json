        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://wdgnllgbfvjgurbqhfqb.supabase.co https://api.anthropic.com https://api.openai.com https://api.transak.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https://wdgnllgbfvjgurbqhfqb.supabase.co wss://wdgnllgbfvjgurbqhfqb.supabase.co https://api.anthropic.com https://api.openai.com https://api.transak.com https://vercel.live; frame-ancestors 'none';"
        }
