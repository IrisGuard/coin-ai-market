{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api|assets|_next|favicon.ico|robots.txt|.*\\.(png|jpg|jpeg|gif|svg|ico|css|js)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://api.anthropic.com https://api.openai.com https://cdn.gpteng.co https://global.transak.com https://staging-global.transak.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com https://global.transak.com https://staging-global.transak.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: blob: https: https://global.transak.com https://staging-global.transak.com; connect-src 'self' https://wdgnllgbfvjgurbqhfqb.supabase.co https://api.anthropic.com https://api.openai.com https://api.transak.com https://staging-api.transak.com https://global.transak.com https://staging-global.transak.com wss://wdgnllgbfvjgurbqhfqb.supabase.co; frame-src 'self' https://global.transak.com https://staging-global.transak.com; frame-ancestors 'none';"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        },
        {
          "key": "X-Auth-Session-Timeout",
          "value": "86400"
        },
        {
          "key": "X-OTP-Expiry",
          "value": "600"
        }
      ]
    }
  ],
  "env": {
